# Prototype environment variables
# Copy to .env and fill in values

# ----- Keycloak -----
# Keycloak PostgreSQL password
KC_DB_PASSWORD=change-me-strong-password

# Keycloak admin password
KC_ADMIN_PASSWORD=change-me-admin-password

# Keycloak client secret (get from Keycloak admin after creating client)
OAUTH_CLIENT_SECRET=your-keycloak-client-secret-here

# ----- LiteLLM -----
# Anthropic API key for Claude
ANTHROPIC_API_KEY=your-anthropic-api-key-here

# MiniMax API key (optional)
MINIMAX_API_KEY=your-minimax-api-key-here

# LiteLLM master key for admin operations and Open WebUI auth
LITELLM_MASTER_KEY=sk-change-me-to-random-string

# LiteLLM database password (optional, defaults to 'litellm')
LITELLM_DB_PASSWORD=litellm

# ----- PFN MCP Server -----
# Valkyrie database connection for energy monitoring
PFN_DATABASE_URL=postgresql://postgres:password@host:5432/valkyrie

# ----- Open WebUI -----
# Random secret for Open WebUI session encryption
WEBUI_SECRET_KEY=change-me-to-random-string

# ----- Keycloak to Valkyrie Sync Script -----
# Keycloak admin API settings
KEYCLOAK_URL=https://auth.forsanusa.id
KEYCLOAK_REALM=pfn
KEYCLOAK_ADMIN_USER=admin
KEYCLOAK_ADMIN_PASSWORD=change-me-admin-password

# Valkyrie database connection
VALKYRIE_DATABASE_URL=postgresql://postgres:password@88.222.213.96:5432/valkyrie
